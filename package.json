{
    "name": "kubernetes-typescript",
    "scripts": {
        "build": "rm -rf dist/ && ./node_modules/typescript/bin/tsc && ./node_modules/prettier/bin-prettier.js --write dist/**/*.js",
        "lint": "./node_modules/eslint/bin/eslint.js ./**/*.ts --fix && ./node_modules/prettier/bin-prettier.js --write ./**/*.ts"
    },
    "devDependencies": {
        "@types/node": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^3.3.0",
        "@typescript-eslint/parser": "^3.3.0",
        "eslint": "^7.0.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-config-typescript": "^3.0.0",
        "eslint-plugin-prettier": "^3.1.2",
        "prettier": "^2.0.5"
    },
    "dependencies": {
        "@pulumi/pulumi": "^2.0.0",
        "@pulumi/kubernetes": "^2.0.0",
        "@pulumi/kubernetesx": "^0.1.1"
    }
}
